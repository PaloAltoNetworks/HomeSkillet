<rules>
  <entry name="block-quic">
    <to>
      <member>{{ ZONE_UNTRUST }}</member>
    </to>
    <from>
      <member>{{ ZONE_TRUST }}</member>
    </from>
    <source>
      <member>any</member>
    </source>
    <destination>
      <member>any</member>
    </destination>
    <source-user>
      <member>any</member>
    </source-user>
    <category>
      <member>any</member>
    </category>
    <application>
      <member>quic</member>
    </application>
    <service>
      <member>application-default</member>
    </service>
    <hip-profiles>
      <member>any</member>
    </hip-profiles>
    <action>deny</action>
    <description>blocks quic and forces use of standard ssl for web sessio
ns to help capture url values of encrypted sessions</description>
    <log-setting>default</log-setting>
     <tag>
       <member>Gold</member>
     </tag>
  </entry>
      <entry name="MSSP-no-unknown-URL-xfer">
        <profile-setting>
          <group>
            <member>Outbound-Unknown-URL</member>
          </group>
        </profile-setting>
        <to>
          <member>{{ ZONE_UNTRUST }}</member>
        </to>
        <from>
          <member>{{ ZONE_TRUST }}</member>
        </from>
        <source>
          <member>any</member>
        </source>
        <destination>
          <member>any</member>
        </destination>
        <source-user>
          <member>any</member>
        </source-user>
        <category>
          <member>unknown</member>
        </category>
        <application>
          <member>any</member>
        </application>
        <service>
          <member>application-default</member>
        </service>
        <hip-profiles>
          <member>any</member>
        </hip-profiles>
        <action>allow</action>
        <log-end>yes</log-end>
        <log-setting>default</log-setting>
        <description>This rule prevents the download of WF supported and risky file types from unknown category
URL's.</description>
         <tag>
           <member>Gold</member>
         </tag>
      </entry>
      <entry name="MSSP-allow-outbound">
        <to>
          <member>{{ ZONE_UNTRUST }}</member>
        </to>
        <from>
          <member>{{ ZONE_TRUST }}</member>
        </from>
        <source>
          <member>any</member>
        </source>
        <destination>
          <member>any</member>
        </destination>
        <source-user>
          <member>any</member>
        </source-user>
        <category>
          <member>any</member>
        </category>
        <application>
          <member>any</member>
        </application>
        <service>
          <member>application-default</member>
        </service>
        <hip-profiles>
          <member>any</member>
        </hip-profiles>
        <action>allow</action>
        <log-end>yes</log-end>
        <log-setting>default</log-setting>
        <profile-setting>
          <group>
            <member>Outbound</member>
          </group>
        </profile-setting>
        <description>This is a general rule to allow trust to untrust traffic. It uses application defaults to p
revent applications from running on non-default undefined ports. This is part of the App-ID support in the firew
all</description>
         <tag>
           <member>Gold</member>
         </tag>
      </entry>
      <entry name="MSSP-non-def-SSL-ports">
        <to>
          <member>{{ ZONE_UNTRUST }}</member>
        </to>
        <from>
          <member>{{ ZONE_TRUST }}</member>
        </from>
        <source>
          <member>any</member>
        </source>
        <destination>
          <member>any</member>
        </destination>
        <source-user>
          <member>any</member>
        </source-user>
        <category>
          <member>any</member>
        </category>
        <application>
          <member>ssl</member>
        </application>
        <service>
          <member>any</member>
        </service>
        <hip-profiles>
          <member>any</member>
        </hip-profiles>
        <action>allow</action>
        <log-end>yes</log-end>
        <log-setting>default</log-setting>
        <profile-setting>
          <group>
            <member>Outbound</member>
          </group>
        </profile-setting>
        <description>This is a complement to the allow-outbound rule to ensure sessions can be established and t
he proper application determined. Once determined, the session will shift to the allow-outbound rule.</description>
         <tag>
           <member>Gold</member>
         </tag>
      </entry>
      <entry name="MSSP-non-def-web-ports">
        <to>
          <member>{{ ZONE_UNTRUST }}</member>
        </to>
        <from>
          <member>{{ ZONE_TRUST }}</member>
        </from>
        <source>
          <member>any</member>
        </source>
        <destination>
          <member>any</member>
        </destination>
        <source-user>
          <member>any</member>
        </source-user>
        <category>
          <member>any</member>
        </category>
        <application>
          <member>web-browsing</member>
        </application>
        <service>
          <member>any</member>
        </service>
        <hip-profiles>
          <member>any</member>
        </hip-profiles>
        <action>allow</action>
        <log-end>yes</log-end>
        <log-setting>default</log-setting>
        <profile-setting>
          <group>
            <member>Outbound</member>
          </group>
        </profile-setting>
        <description>Provide visibility for non-default web browsing applications. Will provide security profile
 coverage and will allow based if a non-default port for the application. VISIBILITY ONLY and not app evasion.</description>
         <tag>
           <member>Gold</member>
         </tag>
      </entry>
      <entry name="MSSP-find-non-def-apps">
        <to>
          <member>{{ ZONE_UNTRUST }}</member>
        </to>
        <from>
          <member>{{ ZONE_TRUST }}</member>
        </from>
        <source>
          <member>any</member>
        </source>
        <destination>
          <member>any</member>
        </destination>
        <source-user>
          <member>any</member>
        </source-user>
        <category>
          <member>any</member>
        </category>
        <application>
          <member>any</member>
        </application>
        <service>
          <member>any</member>
        </service>
        <hip-profiles>
          <member>any</member>
        </hip-profiles>
        <action>allow</action>
        <profile-setting>
          <group>
            <member>Outbound</member>
          </group>
        </profile-setting>
        <description>This is a temporary safety net rule to find apps not on non-default ports. It still has the
 Gold security profile to look for threats, block bad URL's, and send files to WF.</description>
          <tag>
           <member>Gold</member>
         </tag>
        <log-end>yes</log-end>
        <log-setting>default</log-setting>
      </entry>
    </rules>