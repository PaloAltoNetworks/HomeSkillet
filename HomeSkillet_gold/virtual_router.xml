<entry name="default">
  <protocol>
    <bgp>
      <enable>no</enable>
      <dampening-profile>
        <entry name="default">
          <cutoff>1.25</cutoff>
          <reuse>0.5</reuse>
          <max-hold-time>900</max-hold-time>
          <decay-half-life-reachable>300</decay-half-life-reachable>
          <decay-half-life-unreachable>900</decay-half-life-unreachable>
          <enable>yes</enable>
        </entry>
      </dampening-profile>
      <routing-options>
        <graceful-restart>
          <enable>yes</enable>
        </graceful-restart>
      </routing-options>
    </bgp>
  </protocol>
  <interface>
    <member>{{ INTF_UNTRUST }}</member>
    <member>{{ INTF_TRUST }}</member>
  </interface>
  <ecmp>
    <algorithm>
      <ip-modulo/>
    </algorithm>
  </ecmp>
</entry>
